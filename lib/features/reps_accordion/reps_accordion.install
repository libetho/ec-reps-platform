<?php
/**
 * @file
 * Install and setup accordion module.
 */

/**
 * Implements hook_enable().
 */
function reps_accordion_enable() {
  drupal_set_message(t('REPS accordion feature is now active on your site.'));
}

/**
 * Implements hook_disable().
 */
function reps_accordion_disable() {
  drupal_set_message(t('REPS accordion feature is now disabled on your site.'));
}
